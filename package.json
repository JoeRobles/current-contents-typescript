{
  "name": "current-contents",
  "version": "1.0.0",
  "description": "Authors and publications database",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "clear": "rm -rf dist/ coverage/ .nyc_output/",
    "coverage": "nyc npm run test && nyc report --reporter=html",
    "coveralls": "nyc npm run test && nyc report --reporter=text-lcov | coveralls -v",
    "dev": "ts-node ./src/server.ts",
    "start": "nodemon ./dist/server.js",
    "prod": "npm run build && npm run start",
    "test": "mocha -r ts-node/register src/**/*.spec.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JoeRobles/current-contents.git"
  },
  "keywords": [
    "authors",
    "books",
    "database",
    "search"
  ],
  "author": "Joe Robles <joe.robles.pdj@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JoeRobles/current-contents/issues"
  },
  "homepage": "https://github.com/JoeRobles/current-contents#readme",
  "dependencies": {
    "@types/express": "^4.17.1",
    "@types/uniqid": "^4.1.3",
    "aws-sdk": "^2.517.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "mongoose": "^5.7.5",
    "nodemon": "^1.19.1",
    "uniqid": "^5.0.3"
  },
  "devDependencies": {
    "@types/jasmine": "^3.4.0",
    "@types/lodash": "^4.14.137",
    "@types/node": "^12.7.2",
    "@types/supertest": "^2.0.8",
    "aws-sdk-mock": "^4.5.0",
    "chai": "^4.2.0",
    "coveralls": "^3.0.6",
    "jasmine-console-reporter": "^3.1.0",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "sinon": "^7.4.1",
    "supertest": "^4.0.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  }
}
